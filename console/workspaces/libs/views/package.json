{
  "name": "@agent-management-platform/views",
  "version": "1.0.0",
  "type": "module",
  "import": "./dist/index.js",
  "require": "./dist/index.js",
  "types": "dist/index.d.ts",
  "baseUrl": ".",
  "sideEffects": [
    "**/*.*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint --config ./eslint.config.js",
    "test": "vitest run",
    "test-watch": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "copy-assets": "find src -type f \\( -name '*.css' -o -name '*.scss' -o -name '*.less' -o -name '*.woff*' -o -name '*.ttf' -o -name '*.eot' \\) -exec sh -c 'mkdir -p dist/$(dirname {}| sed \"s|^src||\") && cp {} dist/$(dirname {}| sed \"s|^src||\")/$(basename {})' \\;",
    "build": "rushx clean && tsc -project tsconfig.lib.json && rushx copy-assets",
    "dev": "rushx clean && tsc -project tsconfig.lib.json  --watch && rushx copy-assets",
    "i18n": "formatjs extract 'src/**/*.{ts,tsx,js,jsx}' --out-file 'lang/en.json' --ignore '**/node_modules/**' --ignore '**/*.d.ts' --ignore '**/vite-env.d.ts' --format simple --throws"
  },
  "keywords": ["ai", "agents", "ui", "components", "mui", "react"],
  "author": "",
  "license": "Apache-2.0 license",
  "description": "Common UI components library for AI Agent Management Platform",
  "dependencies": {
    "clsx": "2.1.1",
    "@fontsource/roboto": "5.2.5",
    "react-dom": "19.1.1",
    "react-router-dom": "6.28.0",
    "react-intl": "7.1.11",
    "react-hook-form": "7.65.0",
    "dayjs": "1.11.18",
    "@mui/material": "7.0.0",
    "@mui/icons-material": "7.0.0",
    "@emotion/react": "11.13.5",
    "@emotion/styled": "11.13.5",
    "@wso2/oxygen-ui": "0.0.1-alpha.9",
    "@wso2/oxygen-ui-icons-react": "0.0.1-alpha.9",
    "@mui/x-charts": "8.14.0",
    "@mui/x-data-grid": "8.16.0",
    "@mui/x-date-pickers": "8.16.0",
    "@mui/x-tree-view": "8.14.0",
    "date-fns": "4.1.0",
    "lucide-react": "0.545.0",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.6.13",
    "@storybook/addon-interactions": "8.6.13",
    "@storybook/addon-onboarding": "8.6.13",
    "@storybook/blocks": "8.6.13",
    "@storybook/react": "8.6.13",
    "@storybook/react-vite": "8.6.13",
    "@storybook/test": "8.6.13",
    "@testing-library/jest-dom": "6.6.3",
    "@types/jest": "29.5.14",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.33.1",
    "@typescript-eslint/parser": "8.33.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-storybook": "0.12.0",
    "jsdom": "26.1.0",
    "postcss": "8.5.3",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.5.3",
    "react": "19.1.1",
    "storybook": "8.6.13",
    "typescript": "5.9.3",
    "vite": "6.3.5",
    "@vitejs/plugin-react": "5.0.4",
    "vite-plugin-css-modules": "0.0.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.1.3",
    "eslint-import-resolver-typescript": "4.4.1",
    "@eslint/js": "9.36.0",
    "typescript-eslint": "8.45.0",
    "eslint-plugin-testing-library": "7.4.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-import": "2.31.0",
    "globals": "16.4.0",
    "glob": "10.3.10",
    "lodash": "4.17.21",
    "@types/lodash": "4.17.17",
    "@formatjs/cli": "6.7.2",
    "storybook-dark-mode": "4.0.2",
    "@testing-library/dom": "10.4.0",
    "@babel/generator": "7.27.3",
    "@babel/parser": "7.27.3",
    "@babel/traverse": "7.27.3",
    "less": "4.3.0",
    "@eslint/eslintrc": "3.3.1",
    "@agent-management-platform/eslint-config": "workspace:*",
    "@agent-management-platform/types": "workspace:*"
  },
  "peerDependencies": {
    "react": "19.1.1",
    "@types/lodash": "4.17.17",
    "lodash": "4.17.21",
    "@babel/runtime-corejs3": "7.11.2"
  }
}
